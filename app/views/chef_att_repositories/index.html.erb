<p id="notice"><%= notice %></p>

<h1>Listing Chef Att Repositories</h1>

<table>
  <thead>
    <tr>
      <th>Room category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chef_att_repositories.each do |chef_att_repository| %>
      <tr>
        <td><%= chef_att_repository.room_category %></td>
        <td><%= link_to 'Show', chef_att_repository %></td>
        <td><%= link_to 'Edit', edit_chef_att_repository_path(chef_att_repository) %></td>
        <td><%= link_to 'Destroy', chef_att_repository, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Chef att repository', new_chef_att_repository_path %>
